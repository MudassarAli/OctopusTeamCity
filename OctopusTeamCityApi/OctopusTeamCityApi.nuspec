<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
    <id>OctopusTeamCityApi</id>
    <version>1.0.0</version>
    <authors>Mudassar Ali</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Route Debugger is a little utility I wrote...</description>
    </metadata>
    <files>
    		<file src="bin\**\*.*" target="bin" exclude="bin\*.xml" />
		<file src="Content\**\*.*" target="Content" />
		<file src="Areas\**\*.*" target="Areas" />
		<file src="Scripts\**\*.*" target="Scripts" />
		<file src="fonts\**\*.*" target="fonts" />
		<file src="Views\**\*.*" target="Views" />
		<file src="ApplicationInsights.config" />
		<file src="Web.config" />
		<file src="packages.config" />
		<file src="Global.asax" />
    </files>
</package>